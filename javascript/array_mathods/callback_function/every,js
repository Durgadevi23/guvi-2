const everyfunc=(arr,callback)=>{
  var  s=[];
  for(var i=0;i<arr.length;i++)
    {
      if(call(arr[i]))
        {
          s.push(arr[i]);
        }
    }
  if(s.length===arr.length)
    {
    return  true
    }
  else
    return  false
}
var cond=(x)=>x>=18;
var a= [34,55,78,56];
console.log(everyfunc(a,cond))
